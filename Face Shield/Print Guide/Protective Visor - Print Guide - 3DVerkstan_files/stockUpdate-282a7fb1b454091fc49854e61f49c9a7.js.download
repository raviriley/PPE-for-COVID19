jQuery(document).on('ready',function(){jQuery(".select_option").on('click',function(){parent=jQuery(this).closest(".entry-header");stockElement=parent.find(".check-is-correct-stock-val");varID=stockElement.data("var-id");if(varID){updateStockText(".check-is-correct-stock-val[data-var-id='"+varID+"']");jQuery(".check-is-correct-stock-val[data-var-id='"+varID+"']").html("...");jQuery(".check-is-correct-stock-val[data-var-id='"+varID+"']").fadeOut(1000);jQuery(".check-is-correct-stock-val[data-var-id='"+varID+"']").fadeIn(1000)}})});function updateStockText(selector){var idsArray=[];jQuery(selector).each(function(index){varID=jQuery(this).data("var-id");idsArray.push(varID)});if(!jQuery.isEmptyObject(idsArray)){jQuery.ajax({url:stock_update_ajax_obj.ajaxurl,data:{'action':'stock_update_ajax_request','ids':idsArray},success:function(data){jQuery.each(data,function(index,value){jQuery(".check-is-correct-stock-val[data-var-id='"+index+"']").html(value)});return!1}})}}